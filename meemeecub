if game.PlaceId == 221718525 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local playerHead = game.Players.LocalPlayer.Character.Head
local Window = OrionLib:MakeWindow({Name = "meme hub | Ninja Tycoon", HidePremium = false, SaveConfig = true, ConfigFolder = "memecubb", IntroText = "Ninja Tycoon"})




getgenv().Team = "Hyago Clan"
getgenv().collect = true
getgenv().clickio = true

-- functionss
function Coolect()
    while wait() do
        if not getgenv().collect then break end
        for i, v in pairs(game:GetService("Workspace")["Zednov's Tycoon Kit"].Tycoons[getgenv().Team].Essentials.Giver:GetChildren()) do
            if v.Name == "TouchInterest" and v.Parent then
                firetouchinterest(playerHead, v.Parent, 0)
                wait(0.001)
                firetouchinterest(playerHead, v.Parent, 1)
                break;
             end
         end        
    end
end

function C1ckio()
    while clickio == true do
      fireclickdetector(getgenv().Team.PurchasedObjects.Mine.Clicker.ClickDetector)
      wait(0.00000000000000000000000000000001)
     end
    end





-- Tabs and shit
local MTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MTab:AddDropdown({
	Name = "Choose Tycoon",
	Default = "Hyago Clan",
	Options = {"Hyago Clan", "Sonju Clan", "Akatsuke Clan", "Uchika Clan", "Kazeka Clan", "Otsusushi Clan", "Sarukobi Clan", "Uzumonki Clan", "Hotate Clan"},
	Callback = function(Value)
		getgenv().Team = Value
                print(getgenv().Team)
	end    
})

MTab:AddToggle({
	Name = "Auto Collect Cash",
	Default = false,
	Callback = function(Value)
		getgenv().collect = true
                Coolect()
	end    
})

MTab:AddToggle({
	Name = "Auto Click Dropper",
	Default = false,
	Callback = function(Value)
		getgenv().clickio = true
                C1ckio()
	end    
})











end
OrionLib:Init()
